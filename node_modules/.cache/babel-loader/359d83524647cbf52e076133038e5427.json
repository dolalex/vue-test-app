\n              v-model=\"vendor\"\n          >\n          </v-text-field>\n          <v-text-field\n              name=\"color\"\n              label=\"Color Product\"\n              type=\"text\"\n              v-model=\"color\"\n          >\n          </v-text-field>\n          <v-text-field\n              name=\"material\"\n              label=\"Material Product\"\n              type=\"text\"\n              v-model=\"material\"\n          >\n          </v-text-field>\n          <v-text-field\n              name=\"price\"\n              label=\"Price Product\"\n              type=\"text\"\n              v-model=\"price\"\n              :rules=\"[v => !!v || 'Price is required' ]\"\n              required\n          >\n          </v-text-field>\n          <v-text-field\n              name=\"description\"\n              label=\"Description Product\"\n              type=\"text\"\n              multi-line\n              v-model=\"description\"\n          >\n          </v-text-field>\n        </v-form>\n        <v-layout class=\"mb-3\">\n          <v-flex xs12>\n            <v-btn class=\"warning\">\n              Upload\n              <v-icon right dark>cloud_upload</v-icon>\n            </v-btn>\n          </v-flex>\n        </v-layout>\n\n        <v-layout>\n          <v-flex xs12>\n            <img src=\"https://image.ibb.co/fxDsgo/Apple_macbook.jpg\" height=\"200px\" alt=\"\">\n          </v-flex>\n        </v-layout>\n\n        <v-layout>\n          <v-flex xs12>\n            <v-switch\n                color=\"primary\"\n                label=\"Add to Promo?\"\n                v-model=\"promo\"\n            ></v-switch>\n          </v-flex>\n        </v-layout>\n\n        <v-layout>\n          <v-flex xs12>\n            <v-spacer></v-spacer>\n            <v-btn :disabled=\"!valid\" class=\"success\" @click=\"createProduct\">Create New Product</v-btn>\n          </v-flex>\n        </v-layout>\n\n      </v-flex>\n    </v-layout>\n  </v-container>\n</template>\n\n<