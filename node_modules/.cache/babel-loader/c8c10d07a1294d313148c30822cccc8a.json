ng;\n    },\n    isOwner: function isOwner() {\n      return this.product.ownerId === this.$store.getters.user.id;\n    }\n  },\n  components: {\n    appEditProduct: EditProduct\n  }\n};",{"version":3,"sources":["Product.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyCA,OAAA,WAAA,MAAA,2BAAA;AAEA,eAAA;AAEA,EAAA,KAAA,EAAA,CAAA,IAAA,CAFA;AAGA,EAAA,QAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,UAAA,EAAA,GAAA,KAAA,EAAA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,WAAA,CAAA,EAAA,CAAA;AACA,KAJA;AAKA,IAAA,OALA,qBAKA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,OAAA;AACA,KAPA;AAQA,IAAA,OARA,qBAQA;AACA,aAAA,KAAA,OAAA,CAAA,OAAA,KAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,EAAA;AACA;AAVA,GAHA;AAeA,EAAA,UAAA,EAAA;AACA,IAAA,cAAA,EAAA;AADA;AAfA,CAAA","sourcesContent":["<template>\n  <v-container>\n    <section class=\"product mt-3 elevation-10\" v-if=\"!loading\">\n      <v-layout row wrap>\n        <v-flex xs12 lg6>\n          <v-img :src=\"product.imageSrc\" class=\"product-image\" alt=\"\"></v-img>\n        </v-flex>\n        <v-flex xs12 lg6>\n          <div class=\"product-info\">\n            <h5 class=\"product-title display-1 mb-3 mt-3\">{{product.title}}</h5>\n            <p><span>Vendor:</span> {{product.vendor.charAt(0).toUpperCase() + product.vendor.substr(1)}}</p>\n            <p><span>Price:</span> ${{product.price}}</p>\n            <p><span>Color:</span> <span :title=\"purple\" :style=\"{backgroundColor: product.color}\" class=\"product-color-bg\"></span></p>\n            <p><span>Material:</span> {{product.material}}</p>\n            <div>\n              <p><span>Description:</span> {{product.description}}</p>\n            </div>\n            <app-edit-product :product=\"product\" v-if=\"isOwner\"></app-edit-product>\n            <app-buy-dialog :product=\"product\"></app-buy-dialog>\n          </div>\n        </v-flex>\n      </v-layout>\n    </section>\n    <section v-else class=\"text-xs-center\">\n      <v-container>\n        <v-layout row>\n          <v-flex xs12 class=\"text-xs-center pt-5\">\n            <v-progress-circular\n                :size=\"70\"\n                :width=\"7\"\n                color=\"blue\"\n                indeterminate\n            ></v-progress-circular>\n          </v-flex>\n        </v-layout>\n      </v-container>\n    </section>\n  </v-container>\n</template>\n\n<script>\n  import EditProduct from '../components/EditProduct'\n\n  export default {\n\n    props: ['id'],\n    computed: {\n      product() {\n          const id = this.id;\n          return this.$store.getters.productById(id)\n      },\n      loading() {\n        return this.$store.getters.loading\n      },\n      isOwner () {\n        return this.product.ownerId === this.$store.getters.user.id\n      }\n    },\n    components: {\n      appEditProduct: EditProduct\n    }\n  }\n</script>\n\n<style scoped>\n  .product {\n    display: flex;\n    align-items: center;\n    flex-wrap: wrap;\n    border: 1px solid #1c77ad;\n    padding: 30px;\n    margin-bottom: 100px;\n  }\n\n  .product-image {\n    height: 100%;\n    width: 550px;\n  }\n\n  .product-info {\n    margin-left: 50px;\n  }\n\n  .product-title {\n    font-size: 24px;\n    font-weight: bold;\n  }\n\n  .product-color-bg {\n    display: inline-block;\n    width: 50px;\n    height: 20px;\n    border: 1px solid purple;\n    border-radius: 10px;\n  }\n\n  p span {\n    font-size: 16px;\n    color: dodgerblue;\n    font-weight: bold;\n  }\n\n</style>"],"sourceRoot":"src/pages"}]}