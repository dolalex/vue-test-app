ditProduct from '../components/EditProduct'\n\n  export default {\n\n    props: ['id'],\n    computed: {\n      product() {\n          const id = this.id;\n          return this.$store.getters.productById(id)\n      },\n      loading() {\n        return this.$store.getters.loading\n      },\n      isOwner () {\n        return this.product.ownerId === this.$store.getters.user.id\n      }\n    },\n    components: {\n      appEditProduct: EditProduct\n    }\n  }\n</script>\n\n<style scoped>\n  .product {\n    display: flex;\n    align-items: center;\n    flex-wrap: wrap;\n    border: 1px solid #1c77ad;\n    padding: 30px;\n    margin-bottom: 100px;\n  }\n\n  .product-image {\n    height: 100%;\n    width: 550px;\n  }\n\n  .product-info {\n    margin-left: 50px;\n  }\n\n  .product-title {\n    font-size: 24px;\n    font-weight: bold;\n  }\n\n  .product-color-bg {\n    display: inline-block;\n    width: 50px;\n    height: 20px;\n    border: 1px solid purple;\n    border-radius: 10px;\n  }\n\n  p span {\n    font-size: 16px;\n    color: dodgerblue;\n    font-weight: bold;\n  }\n\n</style>"]}]}