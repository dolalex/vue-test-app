                  dark\n                  color=\"light-blue darken-4\"\n                  :disabled=\"localLoading\"\n                  :loading=\"localLoading\"\n              >Buy It</v-btn>\n            </v-card-actions>\n          </v-flex>\n        </v-layout>\n      </v-container>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\n\n  export default {\n    props: ['product'],\n    data() {\n      return {\n        dialog: false,\n        name: '',\n        phone: '',\n        localLoading: false\n      }\n    },\n    methods: {\n      onCancel () {\n        this.name = '',\n        this.phone = '',\n        this.dialog = false\n      },\n      onSave () {\n        if (this.name !== '' && this.phone !== '') {\n          this.localLoading = true\n          this.$store.dispatch('createOrder', {\n            name: this.name,\n            phone: this.phone,\n            productId: this.product.id,\n            ownerId: this.product.ouwnerId\n          }).finally(() => {\n            this.name = ''\n            this.phone = ''\n            this.localLoading = false\n            this.dialog = false\n          })\n          this.dialog = false\n        }\n      }\n    }\n  }\n</script>"]}]}